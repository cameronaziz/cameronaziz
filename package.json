{
  "name": "@cameronaziz/root",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^12.20 || >= 14.13"
  },
  "scripts": {
    "build": "yarn build:app && yarn build:key",
    "build:app": "yarn workspace @cameronaziz/app build",
    "build:key": "yarn workspace @cameronaziz/key build",
    "start": "",
    "clean": "node scripts/clean.js",
    "clean:install": "yarn clean && yarn install",
    "dev": "yarn workspace @cameronaziz/server start --dev",
    "dev:proxy": "yarn workspace @cameronaziz/server start:proxy",
    "dev:app": "yarn workspace @cameronaziz/app dev",
    "dev:key": "yarn workspace @cameronaziz/key dev"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "concurrently": "^6.4.0"
  }
}
