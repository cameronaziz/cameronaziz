    "date-fns": "^2.16.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "prettier-plugin-svelte": "^1.2.1",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.24.1",
    "svelte-check": "^1.0.36",
    "svelte-inline-svg": "^1.0.0",
    "svelte-preprocess": "^4.2.1",
    "tailwindcss": "^1.8.3",
    "typescript": "^4.4.4",
    "vite": "^1.0.0-rc.4",
    "vite-plugin-svelte": "^2.4.1"
  }
}
